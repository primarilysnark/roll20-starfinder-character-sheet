{
  "name": "starfinder-character-sheet",
  "version": "1.0.0",
  "description": "A Starfinder character sheet for Roll20",
  "main": "./bin/sheet.js",
  "scripts": {
    "build": "npm run build-html && npm run build-styles",
    "build-html": "node ./bin/sheet.js",
    "build-styles": "node ./bin/styles.js",
    "lint": "eslint \"./{src,test}/**/*.{mjs,jsx}\"",
    "format": "prettier --write \"./{src,test}/**/*.{mjs,jsx}\"",
    "test": "mocha --loader=testdouble \"./test/**/*.mjs\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primarilysnark/roll20-starfinder-character-sheet.git"
  },
  "author": "PrimarilySnark <starkey.joshua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/primarilysnark/roll20-starfinder-character-sheet/issues"
  },
  "homepage": "https://github.com/primarilysnark/roll20-starfinder-character-sheet#readme",
  "dependencies": {
    "@babel/core": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-transform-roll20": "^1.0.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "lodash": "^4.17.19",
    "node-jsx": "^0.13.3",
    "node-sass": "^7.0.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "mocha": "^8.0.1",
    "should": "^13.2.3",
    "testdouble": "^3.16.1"
  }
}
